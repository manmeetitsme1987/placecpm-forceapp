    <description>This DR ensure that records does not have duplicate PnL Name.</description>
    <duplicateRuleFilter xsi:nil="true"/>
    <duplicateRuleMatchRules>
        <matchRuleSObjectType>P_L_Category__c</matchRuleSObjectType>
        <matchingRule>PnL_Categories_Name_matching_rule</matchingRule>
