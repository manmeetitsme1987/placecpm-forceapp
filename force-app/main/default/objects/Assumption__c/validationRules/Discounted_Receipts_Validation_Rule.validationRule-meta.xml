<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Discounted_Receipts_Validation_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>OR( Discounted_Receipts__c &lt; 0,  Discounted_Receipts__c &gt; 1.0, ISBLANK(Discounted_Receipts__c) )</errorConditionFormula>
    <errorMessage>Discounted Receipts % can not be greater than 100% and less than 0.</errorMessage>
</ValidationRule>
